{
  "name": "@kingsland/open-webui-client",
  "version": "1.3.1",
  "description": "Official OpenWebUI API client for Node.js - TypeScript support with RAG and Ollama proxy endpoints",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "start": "npm run build && node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:legacy": "npm run build && node dist/test.js",
    "example": "npm run build && node dist/example.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "openwebui",
    "open-web-ui",
    "plugin",
    "api",
    "client",
    "typescript",
    "ollama",
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "chat",
    "llm"
  ],
  "author": "kingsland",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kingsland-innovation-center/openwebui-client.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  }
}
