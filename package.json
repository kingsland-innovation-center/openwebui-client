{
  "name": "@kingsland/open-web-ui-client",
  "version": "1.1.0",
  "description": "Official OpenWebUI API client for Node.js - TypeScript support with RAG and Ollama proxy endpoints",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "npm run build && node dist/index.js",
    "test": "npm run build && node dist/test.js",
    "example": "npm run build && node dist/example.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "openwebui",
    "open-web-ui",
    "plugin",
    "api",
    "client",
    "typescript",
    "ollama",
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "chat",
    "llm"
  ],
  "author": "kingsland",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
